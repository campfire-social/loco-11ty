{
  "name": "loco-11ty",
  "version": "1.0.0",
  "description": "11ty HTML initial development for LoCo site",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch src/scss:src/css",
    "build:sass": "sass src/scss:src/css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:eleventy": "npx @11ty/eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/campfire-social/loco-11ty.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/campfire-social/loco-11ty/issues"
  },
  "homepage": "https://github.com/campfire-social/loco-11ty#readme",
  "devDependencies": {
    "@11ty/eleventy-img": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.3",
    "sass-mq": "^6.0.0"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
